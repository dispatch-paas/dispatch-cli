{
  "name": "dispatch-cli",
  "version": "1.0.0",
  "description": "CLI for Dispatch - Safety-first serverless API PaaS",
  "main": "dist/index.js",
  "bin": {
    "dispatch": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "dispatch",
    "cli",
    "openapi",
    "safety",
